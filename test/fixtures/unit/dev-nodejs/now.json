{
    "version": 2,
    "name": "nodejs",
    "builds": [
        { "src": "*.js", "use": "@now/node" },
        { "src": "statics/*", "use": "@now/static" }
    ],
    "routes": [
        { "src": "/api/.*", "dest": "/api.js" },
        { "src": "/help.js", "dest": "/index.js" },
        { "src": "/help", "dest": "/help.js" }
    ]
}
