{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build@canary",
      "config": {
        "distDir": "public"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "xxxxbuildCommand": "rm -Rf public && cp -r src public",
  "xxxxdevCommand": "serve -l $PORT src",
  "routes": [{ "src": "^/api/date$", "dest": "api/date.js" }]
}
