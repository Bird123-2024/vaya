{
  "version": 2,
  "builds": [
    {
      "src": "api/v1/routes/**/*.go",
      "use": "@vercel/go",
      "config": { "zeroConfig": true }
    }
  ],
  "probes": [
    {
      "path": "/api/v1/routes/someroute.go",
      "mustContain": "version:go1.13:Dependency:RANDOMNESS_PLACEHOLDER"
    }
  ]
}
