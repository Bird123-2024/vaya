{
  "name": "now",
  "version": "12.0.0-canary.86",
  "license": "Apache-2.0",
  "description": "The command-line interface for Now",
  "repository": "zeit/now-cli",
  "scripts": {
    "test": "yarn test-lint",
    "test-unit": "ava test/unit.js --fail-fast",
    "test-integration": "ava test/integration.js --serial --fail-fast",
    "test-lint": "eslint .",
    "prepublishOnly": "yarn build-download && cp /dev/null download/dist/now",
    "dev": "webpack -w -d",
    "precommit": "lint-staged",
    "postinstall": "node download/install.js",
    "gzip": "ls packed/now* | xargs gzip",
    "bundle": "pkg dist/now.js -c package.json -o packed/now --options no-warnings",
    "pack": "rm -rf packed && webpack && yarn bundle",
    "build-download": "webpack --context download --config download/webpack.config.js",
    "link": "webpack && cd link && npm link",
    "build": "webpack -p"
  },
  "lint-staged": {
    "*.js": [
      "yarn test-lint",
      "prettier --write --single-quote --no-semi",
      "git add"
    ]
  },
  "bin": {
    "now": "download/dist/now"
  },
  "files": [
    "download/dist",
    "download/install.js"
  ],
  "pkg": {
    "scripts": [
      "dist/*.js"
    ],
    "targets": [
      "node9-alpine-x64",
      "node9-linux-x64",
      "node9-macos-x64",
      "node9-win-x64"
    ]
  },
  "eslintIgnore": [
    "download",
    "dist",
    "test/fixtures"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "flowtype"
    ],
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "func-names": 0,
      "no-console": 0,
      "no-shadow": 0,
      "no-extra-semi": 0,
      "no-empty": 0
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-flow-comments",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-async-generator-functions"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.0.1",
    "@babel/plugin-proposal-async-generator-functions": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-flow-comments": "7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/runtime": "7.0.0",
    "@zeit/dockerignore": "0.0.3",
    "@zeit/source-map-support": "0.6.2",
    "ajv": "6.5.3",
    "alpha-sort": "2.0.1",
    "ansi-escapes": "3.1.0",
    "ansi-regex": "3.0.0",
    "archiver": "3.0.0",
    "arg": "2.0.0",
    "arr-flatten": "1.1.0",
    "async-retry": "1.2.3",
    "async-sema": "2.1.2",
    "ava": "0.25.0",
    "aws-sdk": "2.315.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.0-beta.2",
    "bytes": "3.0.0",
    "chalk": "2.4.1",
    "child-process-promise": "2.2.1",
    "clipboardy": "1.2.3",
    "convert-stream": "1.0.2",
    "copy-webpack-plugin": "4.5.2",
    "credit-card": "3.0.1",
    "death": "1.1.0",
    "debug": "4.0.1",
    "deployment-type": "1.0.3",
    "docker-file-parser": "1.0.3",
    "dotenv": "6.0.0",
    "download": "7.1.0",
    "email-prompt": "0.3.2",
    "email-validator": "2.0.4",
    "epipebomb": "1.0.0",
    "es7-sleep": "1.0.0",
    "eslint": "5.6.0",
    "eslint-plugin-flowtype": "2.50.0",
    "execa": "1.0.0",
    "fetch-h2": "0.2.6",
    "flow-babel-webpack-plugin": "1.1.1",
    "fs-extra": "7.0.0",
    "glob": "7.1.3",
    "ignore": "5.0.2",
    "ini": "1.3.5",
    "inquirer": "6.2.0",
    "is-url": "1.2.4",
    "jsonlines": "0.1.1",
    "lint-staged": "7.2.2",
    "load-json-file": "5.0.0",
    "lodash.range": "3.2.0",
    "micro": "9.3.3",
    "mkdirp-promise": "5.0.1",
    "mri": "1.1.1",
    "ms": "2.1.1",
    "node-fetch": "2.2.0",
    "opn": "5.3.0",
    "ora": "3.0.0",
    "pipe-streams-to-promise": "0.2.0",
    "pkg": "4.3.4",
    "pluralize": "7.0.0",
    "pre-commit": "1.2.2",
    "prettier": "1.14.2",
    "printf": "0.5.1",
    "progress": "2.0.0",
    "psl": "1.1.29",
    "qr-image": "3.2.0",
    "read-pkg": "4.0.1",
    "semver": "5.5.1",
    "shebang-loader": "0.0.1",
    "single-line-log": "1.1.2",
    "sinon": "6.3.3",
    "strip-ansi": "4.0.0",
    "stripe": "6.10.0",
    "supports-color": "5.5.0",
    "tar-fs": "1.16.3",
    "test-listen": "1.1.0",
    "text-table": "0.2.0",
    "then-sleep": "1.0.1",
    "through2": "2.0.3",
    "tmp-promise": "1.0.5",
    "uid-promise": "1.0.0",
    "update-check": "1.5.2",
    "webpack": "4.19.0",
    "webpack-cli": "3.1.0",
    "webpack-node-externals": "1.7.2",
    "which-promise": "1.0.0",
    "write-json-file": "3.0.2"
  }
}
